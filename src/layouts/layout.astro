---
import BaseLayout from './base.astro'
import SEO, { type Props as SEOProps } from './seo.astro'

type Props = SEOProps
const { title, description } = Astro.props
---

<BaseLayout>
  {/* <head /> */}
  <Fragment slot='head'>
    <SEO title={title} description={description} />
    <slot name='head' />
  </Fragment>

  {/* <body /> */}
  <Fragment>
    <slot />
  </Fragment>
</BaseLayout>
